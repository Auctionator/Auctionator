<Ui xmlns="http://www.blizzard.com/wow/ui/">
  <Button
    name="AuctionatorDeleteButtonCellTemplate"
    mixin="AuctionatorDeleteButtonCellTemplateMixin"
    inherits="AuctionatorCell"
    virtual="true"
  >
    <Frames>
      <Button parentKey="DeleteButton" inherits="UIPanelButtonTemplate">
        <Size>
          <AbsDimension x="15" y="15"/>
        </Size>
        <Anchors>
          <Anchor point="TOPRIGHT" x="-10" y="0"/>
        </Anchors>
        <Layers>
          <Layer level="ARTWORK">
            <Texture file="Interface\Buttons\UI-GroupLoot-Pass-Up" alpha="0.75" parentKey="texture"/>
          </Layer>
        </Layers>

        <Scripts>
          <OnClick>self:GetParent():DeleteItem()</OnClick>
        </Scripts>
      </Button>
    </Frames>
  </Button>
</Ui>