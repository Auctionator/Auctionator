<Ui xmlns="http://www.blizzard.com/wow/ui/">
  <Frame
    name="AuctionatorUndercuttingTabFrameTemplate"
    virtual="true"
    inherits="AuctionatorTabFrameTemplate"
    parentKey="AuctionatorUndercuttingFrame"
    mixin="AuctionatorUndercuttingFrameMixin"
  >
    <Scripts>
      <OnLoad method="OnLoad" />
    </Scripts>

    <Frames>
      <Frame parentKey="DataProvider" mixin="UndercuttingDataProviderMixin">
        <Scripts>
          <OnLoad method="OnLoad"/>
          <OnEvent method="OnEvent"/>
          <OnUpdate method="OnUpdate"/>
        </Scripts>
      </Frame>

      <Frame parentKey="ResultsListing" inherits="AuctionatorResultsListingTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="4" y="24"/>
          <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" y="55" />
        </Anchors>
      </Frame>

      <Frame>
        <Size y="55"/>
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent.ResultsListing" relativePoint="BOTTOMLEFT" y="20" x="-1"/>
          <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" />
        </Anchors>
        <Layers>
          <Layer level="BACKGROUND" textureSubLevel="-6">
            <Texture name="$parentBg" file="Interface\FrameGeneral\UI-Background-Rock" horizTile="true" vertTile="true" parentKey="Bg">
              <Anchors>
                <Anchor point="TOPLEFT" y="-21"/>
                <Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
              </Anchors>
            </Texture>
          </Layer>
        </Layers>
      </Frame>
    </Frames>
  </Frame>
</Ui>
