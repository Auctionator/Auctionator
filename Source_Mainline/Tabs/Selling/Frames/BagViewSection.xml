<Ui>
  <Frame name="AuctionatorSellingBagViewSectionTemplate" mixin="AuctionatorBagViewSectionMixin" virtual="true">
    <KeyValues>
      <!--<KeyValue key="rowWidth" value="6" type="number" />-->
      <KeyValue key="collapsable" value="true" type="boolean" />
      <KeyValue key="insetLeft" value="0" type="number" />
      <KeyValue key="sectionTitleHeight" value="22" type="number" />
      <KeyValue key="paddingBottom" value="0" type="number" />
    </KeyValues>
    <Size y="20"/>
    <Frames>
      <Button parentKey="SectionTitle" inherits="AuctionCategoryButtonTemplate">
        <Size y="20"/>
        <Anchors>
          <Anchor point="RIGHT" />
        </Anchors>


        <Scripts>
          <OnLoad>
            AuctionFrameFilter_OnLoad(self)
            self.HighlightTexture:SetAllPoints()
            self.NormalTexture:SetSize(32)
          </OnLoad>
          <OnClick>self:GetParent():ToggleOpen()</OnClick>
        </Scripts>
      </Button>
    </Frames>
  </Frame>
</Ui>
